<!DOCTYPE html>
<html>

<!-- Head Row | Start -->
  <head>
    <title>Nomster</title>
  </head>
<!-- Head Row | End -->

<!-- Body of Web Page -->
    <body>

<!-- Header | Start  -->
      <header>
          <h1> Nomster | Great Places Reviewed </h1>
      </header>
        <br/>
<!-- Header | Stop -->

<!-- Body Content -->

      <% @places.each do |place| %>
        <!--Method instructs app to iterate accross all @place Variable instances-->

        <div class = "opaque_box col-10 offset-1">
          <h2><%= place.name %></h2>
          <i><%= place.address %></i><br/>
          <p><%= place.description %></p><br/>
        </div>
      <% end %>



    </body>
  </html>

